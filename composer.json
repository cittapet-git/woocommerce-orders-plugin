{
    "name": "cittapet/woocommerce-real-time-orders-plugin",
    "description": "Plugin de WordPress para mostrar Ã³rdenes de WooCommerce en tiempo real usando shortcodes",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Cittapet",
            "homepage": "https://github.com/cittapet-git"
        }
    ],
    "keywords": [
        "wordpress",
        "plugin",
        "woocommerce",
        "orders",
        "real-time",
        "shortcode"
    ],
    "homepage": "https://github.com/cittapet-git/woocommerce-real-time-orders-plugin",
    "support": {
        "issues": "https://github.com/cittapet-git/woocommerce-real-time-orders-plugin/issues",
        "source": "https://github.com/cittapet-git/woocommerce-real-time-orders-plugin"
    },
    "require": {
        "php": ">=7.4",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "wp-coding-standards/wpcs": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cittapet\\WooCommerceRealTimeOrders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cittapet\\WooCommerceRealTimeOrders\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress",
        "phpcbf": "phpcbf --standard=WordPress"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "installer-name": "woocommerce-real-time-orders",
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
        }
    }
} 